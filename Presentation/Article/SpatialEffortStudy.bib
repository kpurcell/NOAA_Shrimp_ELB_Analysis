	note = {{ArticleType:} research-article / Full publication date: Oct., 2007 / Copyright © 2007 Coastal and Estuarine Research Federation},
	pages = {791--801},
	file = {JSTOR Full Text PDF:/home/kevin/.mozilla/firefox/4au05rwr.default/zotero/storage/US9TRGP6/Justić et al. - 2007 - Forecasting Gulf's Hypoxia The Next 50 Years.pdf:application/pdf}
